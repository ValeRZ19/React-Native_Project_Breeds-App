{
  "expo": {
    "name": "Pawpedia",
    "slug": "Pawpedia",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/dog-icon.png",
    "scheme": "breedapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#DDE8EE"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.valerzu19.Pawpedia"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/DogLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#DDE8EE"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a46e3e25-8cfe-4c4d-9f4c-0b4de315cfde"
      }
    }
  }
}
